{
    "version": 2,
    "builds": [{ "src": "app.py", "use": "@vercel/python" }],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/tmp/(.*)",
        "headers": { "Cache-Control": "no-store" },
        "continue": true
      },
      { "src": "/.*", "dest": "app.py" }
    ],
    "env": {
      "MPLCONFIGDIR": "/tmp/matplotlib-cache"
    }
}